@model IEnumerable<StudentApplicationGuidance.Data.ApplicationUser>

@{
    ViewData["Title"] = "System Users";
}

<h2>@ViewData["Title"]</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Username</th>
            <th>Email</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Province</th>
            <th>Funding Source</th>
            <!-- Add other properties as needed -->
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>
                <td>@user.Email</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Province?.Name</td>
                <td>@user.FundingSource?.Name</td> 
                
            </tr>
        }
    </tbody>
</table>
